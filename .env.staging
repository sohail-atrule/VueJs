NODE_ENV=staging
VITE_APP_MODE=staging

# API Configuration
VITE_APP_API_URL=https://staging-api.serviceprovider.com
VITE_APP_API_TIMEOUT=30000

# Azure AD B2C Authentication Configuration
VITE_APP_AUTH_AUTHORITY=https://login.microsoftonline.com/staging-tenant-id
VITE_APP_AUTH_CLIENT_ID=staging-azure-ad-b2c-client-id
VITE_APP_AUTH_SCOPE=api://staging-serviceprovider/.default
VITE_APP_AUTH_REDIRECT_URI=https://staging.serviceprovider.com/auth-callback
VITE_APP_AUTH_SILENT_REDIRECT_URI=https://staging.serviceprovider.com/silent-refresh

# OneDrive Integration
VITE_APP_ONEDRIVE_CLIENT_ID=staging-onedrive-client-id
VITE_APP_ONEDRIVE_SCOPE=files.readwrite.all

# Error Reporting & Monitoring
VITE_APP_ENABLE_ERROR_REPORTING=true
VITE_APP_ERROR_REPORTING_KEY=staging-application-insights-key

# Cache Configuration
VITE_APP_CACHE_TTL=3600

# File Upload Limits
VITE_APP_MAX_UPLOAD_SIZE=10485760

# Environment Labeling
VITE_APP_ENVIRONMENT_LABEL=STAGING

# Logging Configuration
VITE_APP_LOG_LEVEL=debug

# Feature Flags
VITE_APP_FEATURE_FLAGS={"enableNewFeatures":true,"enableDebugTools":true}